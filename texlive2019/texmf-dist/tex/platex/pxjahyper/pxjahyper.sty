\ifpxhy@tounicode

  \ifnum\pxhy@driver=\pxhy@driver@dvipdfmx %<*dvipdfmx>
    %% dvipdfmx
    \if e\pxhy@jcode % euc
      \pxhy@out@special{pdf:tounicode EUC-UCS2}
    \else\if s\pxhy@jcode % sjis
