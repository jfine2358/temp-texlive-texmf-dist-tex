\def\mathhexbox#1#2#3{%
  \ifnum"#1<\sixt@@n
    \mbox{$\m@th \mathchar"#1#2#3$}%
  \else
    \mbox{$\m@th \omathchar"#100#2#3$}%
  \fi
}
