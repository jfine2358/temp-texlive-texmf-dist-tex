\DeclareFontShape{T1}{zlmvtt}{l}{it}
     {<->ssub*zlmvtt/l/sl}{}
%
\DeclareFontShape{T1}{zlmvtt}{b}{n}
     {<-> \zlmtt@@scale ec-lmvtk10}{}
\DeclareFontShape{T1}{zlmvtt}{b}{sl}
