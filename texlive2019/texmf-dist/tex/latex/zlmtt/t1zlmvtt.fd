     {<-> \zlmtt@@scale ec-lmvtto10}{}
\DeclareFontShape{T1}{zlmvtt}{m}{it}
     {<->ssub*zlmvtt/m/sl}{}
%
\DeclareFontShape{T1}{zlmvtt}{l}{n}
