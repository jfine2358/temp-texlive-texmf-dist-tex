     {<->ssub*zlmvtt/l/sl}{}
% 
\DeclareFontShape{OT1}{zlmvtt}{b}{n}
     {<-> \zlmtt@@scale rm-lmvtk10}{}
\DeclareFontShape{OT1}{zlmvtt}{b}{sl}
