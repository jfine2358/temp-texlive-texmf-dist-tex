     {<-> \zlmtt@@scale rm-lmvtto10}{}
\DeclareFontShape{OT1}{zlmvtt}{m}{it}
     {<->ssub*zlmvtt/m/sl}{}
%
\DeclareFontShape{OT1}{zlmvtt}{l}{n}
     {<-> \zlmtt@@scale rm-lmvtl10}{}
