  \setlength{\temp}{\Fraiseheight}%
  \addtolength{\temp}{\Flinewidth}%
  \addtolength{\temp}{-\Fnegshort}%
  \rule[\Fnegshort]{\Flinewidth}{\temp}%
  \setlength{\temp}{.5\Fbaselength}%
  \addtolength{\temp}{-0.5\Flinewidth}%
