\ProvidesFile{t2btempora-tosf.fd}
    [2015/09/13  Font definitions for T2B/Tempora-TOSF.]

\expandafter\ifx\csname LGCscale\endcsname\relax
    \let\Tempora@@scale\@empty
\else
    \edef\Tempora@@scale{s*[\csname LGCscale\endcsname]}%
\fi

\DeclareFontFamily{T2B}{Tempora-TOSF}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{b}{it}{
      <-> \Tempora@@scale Tempora-BoldItalic-TOSF-t2b
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{b}{n}{
      <-> \Tempora@@scale Tempora-Bold-TOSF-t2b
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{b}{sl}{
      <-> ssub * Tempora-TOSF/b/it
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{m}{n}{
      <-> \Tempora@@scale Tempora-Regular-TOSF-t2b
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{m}{it}{
      <-> \Tempora@@scale Tempora-Italic-TOSF-t2b
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{m}{sl}{
      <-> ssub * Tempora-TOSF/m/it
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{bx}{n}{
      <-> ssub * Tempora-TOSF/b/n
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{bx}{sl}{
      <-> ssub * Tempora-TOSF/b/sl
}{}

\DeclareFontShape{T2B}{Tempora-TOSF}{bx}{it}{
      <-> ssub * Tempora-TOSF/b/it
}{}

