    \advance\th@mbposy-\th@mbheighty%
    \advance\th@mbposy-\th@mbsdistance%
  \else%
    \advance\th@mbposy-\th@mbheighty%
    \advance\th@mbposy-\th@mbsdistance%
    \thumborigaddthumb{}{}{\string\thepagecolor}{\string\thepagecolor}%
    \th@mb@dvance%
  \fi%
  }

