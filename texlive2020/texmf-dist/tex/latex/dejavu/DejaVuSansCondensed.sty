\ProvidesPackage{DejaVuSansCondensed} [2011/06/01 Style file for DejaVuSansCondensed.]
%%
%% Options:
%%  [scaled]       --> Scale = 0.95
%%  [scaled=0.9]   --> Scale = 0.90
%%
\RequirePackage{keyval}
\define@key{DejaVuSansCondensed}{scaled}[0.95]{%
  \def\DejaVuSansCondensed@scale{#1}}
\def\ProcessOptionsWithKV#1{%
  \let\@tempc\relax
  \let\DejaVuSansCondensed@tempa\@empty
  \@for\CurrentOption:=\@classoptionslist\do{%
    \@ifundefined{KV@#1@\CurrentOption}%
    {}%
    {%
      \edef\DejaVuSansCondensed@tempa{\DejaVuSansCondensed@tempa,\CurrentOption,}%
      \@expandtwoargs\@removeelement\CurrentOption
        \@unusedoptionlist\@unusedoptionlist
    }%
  }%
  \edef\DejaVuSansCondensed@tempa{%
    \noexpand\setkeys{#1}{%
      \DejaVuSansCondensed@tempa\@ptionlist{\@currname.\@currext}%
    }%
  }%
  \DejaVuSansCondensed@tempa
  \let\CurrentOption\@empty
}
\ProcessOptionsWithKV{DejaVuSansCondensed}
\AtEndOfPackage{%
  \let\@unprocessedoptions\relax
}
    

\renewcommand{\sfdefault}{DejaVuSansCondensed-TLF}

\endinput
