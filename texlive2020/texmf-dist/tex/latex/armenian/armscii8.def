%% Providing a command to define armew.
%%
\def\definearmew@error{%
  \PackageError{armtex}
    {Character\space used\space to\space define\space armew\space must\MessageBreak
     have\space a\space charcode\space between\space 128\space and\space 255}
    {Change\space the\space character\space used\space in\space
     \protect\definearmew\space and\space recompile.}}

\providecommand\definearmew[1]{%
  \count@=`#1\relax
  \ifnum \count@ < 128\definearmew@error\else
  \ifnum \count@ > 255\definearmew@error\else
  \DeclareInputText{\count@}{\armew}\fi\fi}


