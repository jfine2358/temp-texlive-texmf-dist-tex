% Ce package fournit la commande \psvectorian[options]{num}
% qui s'utilise ainsi: \rput[refpoint]{angle}(x,y){\psvectorian[options]{num}}
%
% Les options de \psvectorian sont:
%
% scale = nombre (échelle, 1 par défaut)
% opacity = nombre entre 0 et 1 (opacité, 1 par défaut)
% width = nombre+unité (largeur, naturelle par défaut)
% height = nombre+unité (hauteur, naturelle par défaut)
% color = couleur (\psvectorianDefaultColor par défaut)
% flip = true/false (symétrie horizontale, false par défaut)
% mirror = true/false (symétrie verticale, false par défaut)
%
\NeedsTeXFormat{LaTeX2e}% 
\RequirePackage{graphicx,pstricks}%
\ProvidesPackage{psvectorian}[2012/12/12 v0.4 vector ornaments]%
%
\pst@addfams{psvectorian}%% Herbert V.
\pstheader{psvectorian.pro}%% Herbert V.
%
\newcommand*\psvectorianDefaultColor{black}%couleur par défaut
%
\define@key[psset]{psvectorian}{scale}{\def\@psvectorianBox##1{\scalebox{#1}{##1}}}%
\define@key[psset]{psvectorian}{width}{%
 \def\@psvectorianWidth{#1}%
 \ifx\@psvectorianHeight\empty\def\@psvectorianBox##1{\resizebox*{#1}{!}{##1}}%
 \else\def\@psvectorianBox##1{\resizebox*{#1}{\@psvectorianHeight}{##1}}%
 \fi}%
%
\define@key[psset]{psvectorian}{height}{%
 \def\@psvectorianHeight{#1}%
 \ifx\@psvectorianWidth\empty\def\@psvectorianBox##1{\resizebox*{!}{#1}{##1}}%
 \else\def\@psvectorianBox##1{\resizebox*{\@psvectorianWidth}{#1}{##1}}%
 \fi}%
%
\define@key[psset]{psvectorian}{color}{\def\@psvectorianColor{#1}}%
%
\define@key[psset]{psvectorian}{opacity}{\def\@psvectorianOpacity{#1}}%%
%
\define@boolkey[psset]{psvectorian}[Pst@]{flip}[true]{%
 \ifPst@flip\def\@psvectorianflip##1{\rotatebox{180}{\reflectbox{##1}}}%
 \else      \def\@psvectorianflip##1{##1}%
 \fi}%
%
\define@boolkey[psset]{psvectorian}[Pst@]{mirror}[true]{%
 \ifPst@mirror\let\@psvectorianmirror\reflectbox%
 \else        \def\@psvectorianmirror##1{##1}%
 \fi}
%
\newcommand*\psvectorian[2][]{%
\bgroup%
  \def\@psvectorianWidth{}%
  \def\@psvectorianHeight{}%
  \psset[psvectorian]{scale=1,opacity=1,color=\psvectorianDefaultColor,flip=false,mirror=false}{}%
  \ifx\relax#1\relax\else\psset[psvectorian]{#1}\fi%
  \psset{linecolor=\@psvectorianColor,opacity=\@psvectorianOpacity}%
  \@psvectorianBox{\@psvectorianmirror{\@psvectorianflip{\@psvectorianDraw{#2}}}}%
\egroup%
}%
%
\def\pst@setColor{\pst@usecolor\pslinecolor}%
