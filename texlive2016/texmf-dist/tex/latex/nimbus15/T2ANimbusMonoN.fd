\ProvidesFile{T2ANimbusMonoN.fd}
    [2016/01/08 Font definitions for T2A/NimbusMonoN (msharpe).]

\expandafter\ifx\csname NimbusMonoN@scale\endcsname\relax
    \let\NimbusMonoN@@scale\@empty
\else
    \edef\NimbusMonoN@@scale{s*[\csname NimbusMonoN@scale\endcsname]}%
\fi

\DeclareFontFamily{T2A}{NimbusMonoN}{\hyphenchar\font \m@ne}


\DeclareFontShape{T2A}{NimbusMonoN}{m}{n}{
      <-> \NimbusMonoN@@scale zcoN-Regular-t2a
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{m}{sl}{
      <-> \NimbusMonoN@@scale zcoN-Oblique-t2a
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{m}{it}{
      <-> ssub * NimbusMonoN/m/sl
}{}
\DeclareFontShape{T2A}{NimbusMonoN}{l}{sl}{
      <-> ssub * NimbusMonoN/m/sl
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{l}{n}{
      <-> ssub * NimbusMonoN/m/n
}{}


\DeclareFontShape{T2A}{NimbusMonoN}{b}{sl}{
      <-> ssub * NimbusMonoN/m/sl
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{b}{n}{
      <-> ssub * NimbusMonoN/m/n
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{b}{it}{
      <-> ssub * NimbusMonoN/m/sl
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{bx}{it}{
      <-> ssub * NimbusMonoN/m/sl
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{bx}{sl}{
      <-> ssub * NimbusMonoN/m/sl
}{}

\DeclareFontShape{T2A}{NimbusMonoN}{bx}{n}{
      <-> ssub * NimbusMonoN/m/n
}{}

\endinput
