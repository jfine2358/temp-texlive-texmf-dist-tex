\ProvidesFile{T2CPTSansNarrow-TLF.fd}
    [2014/02/02 Font definitions for T2C/PTSansNarrow-TLF.]

\expandafter\ifx\csname PTSansNarrow@scale\endcsname\relax
    \let\PTSansNarrow@@scale\@empty
\else
    \edef\PTSansNarrow@@scale{s*[\csname PTSansNarrow@scale\endcsname]}%
\fi
    
\DeclareFontFamily{T2C}{PTSansNarrow-TLF}{}

\DeclareFontShape{T2C}{PTSansNarrow-TLF}{m}{n}{
            <->     \PTSansNarrow@@scale PTSans-Narrow-tlf-t2c
}{}

\DeclareFontShape{T2C}{PTSansNarrow-TLF}{b}{n}{
            <->     \PTSansNarrow@@scale PTSans-NarrowBold-tlf-t2c
}{}

\DeclareFontShape{T2C}{PTSansNarrow-TLF}{m}{sl}{
            <->     \PTSansNarrow@@scale PTSans-NarrowSlanted-tlf-t2c
}{}

\DeclareFontShape{T2C}{PTSansNarrow-TLF}{b}{sl}{
            <->     \PTSansNarrow@@scale PTSans-NarrowBoldSlanted-tlf-t2c
}{}


\DeclareFontShape{T2C}{PTSansNarrow-TLF}{bx}{n}{<->ssub * PTSansNarrow-TLF/b/n}{}
\DeclareFontShape{T2C}{PTSansNarrow-TLF}{bx}{sl}{<->ssub * PTSansNarrow-TLF/b/sl}{}


\endinput
