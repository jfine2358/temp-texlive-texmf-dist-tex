}{}

\DeclareFontShape{X2}{PTSansCaption-TLF}{b}{n}{
            <->     \PTSansCaption@@scale PTSans-CaptionBold-tlf-x2
}{}

\DeclareFontShape{X2}{PTSansCaption-TLF}{m}{sl}{
            <->     \PTSansCaption@@scale PTSans-CaptionSlanted-tlf-x2
}{}

\DeclareFontShape{X2}{PTSansCaption-TLF}{b}{sl}{
            <->     \PTSansCaption@@scale PTSans-CaptionBoldSlanted-tlf-x2
}{}


\DeclareFontShape{X2}{PTSansCaption-TLF}{bx}{n}{<->ssub * PTSansCaption-TLF/b/n}{}
\DeclareFontShape{X2}{PTSansCaption-TLF}{bx}{sl}{<->ssub * PTSansCaption-TLF/b/sl}{}


