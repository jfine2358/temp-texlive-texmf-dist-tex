\else
    \edef\PTSansCaption@@scale{s*[\csname PTSansCaption@scale\endcsname]}%
\fi
    
\DeclareFontFamily{X2}{PTSansCaption-TLF}{}

\DeclareFontShape{X2}{PTSansCaption-TLF}{m}{n}{
