  {
    \hbox_set:Nn \l__driver_tmp_box
      {
        \tex_kern:D \__dim_eval:w #6 \__dim_eval_end:
        \__driver_scope_begin:
        \__driver_draw_transformcm:nnnnnn {#2} {#3} {#4} {#5}
          { 0pt } { 0pt }
        \box_move_up:nn {#7} { \box_use:N #1 }
        \__driver_scope_end:
