\newcommand{\definergbcolor}[2]{%
\@ifundefined{rgbcolor@#1}{%
\expandafter\def\csname rgbcolor@#1\endcsname{\addfontfeature{Color=#2}}}{%
\PackageInfo{xecolor}{Redefining color #1}%
\expandafter\def\csname rgbcolor@#1\endcsname{\addfontfeature{Color=#2}}}%
}
\newcommand{\xecolor}[1]{\csname rgbcolor@#1\endcsname}
\newcommand{\textxecolor}[2]{\protect\leavevmode{\xecolor{#1}#2}}
\newcommand{\normalxecolor}{\xecolor{black}}
