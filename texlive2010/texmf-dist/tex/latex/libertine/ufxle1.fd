\ProvidesFile{ufxle1.fd}
   [2010/07/06 scaled font definitions for libertine U/fxle1 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxle1}{}

\DeclareFontShape{U}{fxle1}{m}{n}{
   <-> \fxl@@scale fxlr-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{m}{sl}{
   <-> \fxl@@scale fxlro-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{m}{it}{
   <-> \fxl@@scale fxlri-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{b}{n}{
   <-> \fxl@@scale fxlb-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{b}{sl}{
   <-> \fxl@@scale fxlbo-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{b}{it}{
   <-> \fxl@@scale fxlbi-xl-e1
}{}

\DeclareFontShape{U}{fxle1}{bx}{n}{<->ssub * fxle1/b/n}{}
\DeclareFontShape{U}{fxle1}{bx}{sl}{<->ssub * fxle1/b/sl}{}
\DeclareFontShape{U}{fxle1}{bx}{it}{<->ssub * fxle1/b/it}{}

\endinput
