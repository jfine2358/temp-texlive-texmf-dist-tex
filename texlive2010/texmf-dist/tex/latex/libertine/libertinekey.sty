\ifxetex
  \newfontfamily\biolinumkey[Mapping=tex-text]{Linux Biolinum Kb O}
  \DeclareRobustCommand*\biolinumKeyGlyph[1]{{%
    \biolinumkey%
    \@tempcnta=\XeTeXglyphindex "#1"\relax%
    \ifnum\@tempcnta>0%
      \XeTeXglyph\@tempcnta%
    \fi
  }}
\else
  \def\biolinumkey{\fontfamily{fxk}\selectfont}
  \DeclareRobustCommand*\biolinumKeyGlyph[1]{{\@nameuse{fxk@#1}}}
  %
   \ifluatex
      \input{fxkuni.inc}
   \else
      \input{fxk.inc}
   \fi
  %
\fi
