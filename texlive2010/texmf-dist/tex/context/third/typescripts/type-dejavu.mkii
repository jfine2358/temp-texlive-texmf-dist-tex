%D \module
%D   [       file=type-dejavu,
%D        version=2009.04.02,
%D          title=\CONTEXT\ Typescript Macros,
%D       subtitle=DejaVu Collection,
%D         author=Wolfgang Schuster,
%D           date=\currentdate,
%D      copyright=Wolfgang Schuster]
