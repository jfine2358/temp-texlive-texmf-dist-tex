\setlength{\unitlength}{1.0cm}
\newcommand{\huelle}[4]{%
  \begin{picture}(15.0,15.0)
  \put(1.0,1.0){\framebox(2.5,10.2){\begin{turn}{270}#1\end{turn}}}
  \put(3.5,1.0){\framebox(1.3,10.2){\begin{turn}{270}#2\end{turn}}}
  \put(4.8,1.0){\framebox(6.4,10.2)[#4]{\parbox{6.4cm}{\begin{center}#3\end{center}}}}
  \end{picture}
}

\newcommand{\einleger}[6]{%
  \begin{picture}(15.0,15.0)
  \put(1.0,1.0){\framebox(2.5,10.2){\begin{turn}{270}#1\end{turn}}}
  \put(3.5,1.0){\framebox(1.3,10.2){\begin{turn}{270}#2\end{turn}}}
  \put(4.8,1.0){\framebox(6.4,10.2)[#4]{\parbox{6.4cm}{\begin{center}#3\end{center}}}}
  \put(11.2,1.0){\framebox(6.4,10.2)[#6]{\parbox{6.4cm}{\begin{center}#5\end{center}}}}
  \end{picture}
}

