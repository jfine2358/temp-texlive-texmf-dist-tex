%%
\ProvidesFile{LY1ETbb-TLF.fd}
    [2020/02/15  Font definitions for LY1/ETbb-TLF.]

\expandafter\ifx\csname ETbb@@swashQ\endcsname\relax%
	\global\let\ETbb@@swashQ\@empty
\fi

\ifcsname s@fct@alias\endcsname\else
\gdef\s@fct@alias{\sub@sfcnt\@font@aliasinfo}
\gdef\@font@aliasinfo#1{%
    \@font@info{Font\space shape\space `\curr@fontshape'\space will
        \space be\space aliased\MessageBreak to\space `\mandatory@arg'}%
}
\fi

\edef\ETbb@@S{} % do not use fancy sharpS
\expandafter\ifx\csname @ETbb@sharpSfalse\endcsname\relax
\csname iftrue\endcsname
\else
	\if@ETbb@sharpS\edef\ETbb@@S{1}\fi % do use fancy sharpS
\fi
%\show\ETbb@@S
\expandafter\ifx\csname ETbb@scale\endcsname\relax
    \let\ETbb@@scale\@empty
\else
    \edef\ETbb@@scale{s*[\csname ETbb@scale\endcsname]}%
\fi

\DeclareFontFamily{LY1}{ETbb-TLF}{}


%   ----  bold  ----

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{nw}{
      <-> \ETbb@@scale ETbb-Bold-tlf-swash-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{sw}{
      <-> \ETbb@@scale ETbb-BoldItalic-tlf-swash-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{it}{
      <-> \ETbb@@scale ETbb\ETbb@@S-BoldItalic-tlf\ETbb@@swashQ-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{n}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Bold-tlf\ETbb@@swashQ-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{sc}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Bold-tlf-sc-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{scit}{
      <-> \ETbb@@scale ETbb\ETbb@@S-BoldItalic-tlf-sc-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{scsl}{
      <-> ssub * ETbb-TLF/bold/scit
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{bold}{sl}{
      <-> ssub * ETbb-TLF/bold/it
}{}


%   ----  regular  ----

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{scit}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Italic-tlf-sc-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{sc}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Regular-tlf-sc-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{it}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Italic-tlf\ETbb@@swashQ-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{n}{
      <-> \ETbb@@scale ETbb\ETbb@@S-Regular-tlf\ETbb@@swashQ-ly1
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{scsl}{
      <-> ssub * ETbb-TLF/regular/scit
}{}

\DeclareFontShape{LY1}{ETbb-TLF}{regular}{sl}{
      <-> ssub * ETbb-TLF/regular/it
}{}

