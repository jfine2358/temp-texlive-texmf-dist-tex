\ProvidesFile{X2cantarell-OsF.fd}[2019/06/24 Font definitions for X2/cantarell-OsF.]

\expandafter\ifx\csname cantarell@scale\endcsname\relax
    \let\cantarell@@scale\@empty
\else
    \edef\cantarell@@scale{s*[\csname cantarell@scale\endcsname]}
\fi

\DeclareFontFamily{X2}{cantarell-OsF}{}

\DeclareFontShape{X2}{cantarell-OsF}{l}{n}{<-> \cantarell@@scale Cantarell-Light-X2-OsF}{}
