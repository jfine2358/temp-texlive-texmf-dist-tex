\ProvidesFile{X2cantarell-LF.fd}[2019/06/24 Font definitions for X2/cantarell-LF.]

\expandafter\ifx\csname cantarell@scale\endcsname\relax
    \let\cantarell@@scale\@empty
\else
    \edef\cantarell@@scale{s*[\csname cantarell@scale\endcsname]}
\fi

\DeclareFontFamily{X2}{cantarell-LF}{}

\DeclareFontShape{X2}{cantarell-LF}{l}{n}{<-> \cantarell@@scale Cantarell-Light-X2-LF}{}
\DeclareFontShape{X2}{cantarell-LF}{b}{n}{<-> \cantarell@@scale Cantarell-Bold-X2-LF}{}
\DeclareFontShape{X2}{cantarell-LF}{el}{n}{<-> \cantarell@@scale Cantarell-Thin-X2-LF}{}
\DeclareFontShape{X2}{cantarell-LF}{m}{n}{<-> \cantarell@@scale Cantarell-Regular-X2-LF}{}
\DeclareFontShape{X2}{cantarell-LF}{eb}{n}{<-> \cantarell@@scale Cantarell-ExtraBold-X2-LF}{}
\DeclareFontShape{X2}{cantarell-LF}{l}{sl}{<-> \cantarell@@scale Cantarell-Light-X2-LF-Slanted}{}
\DeclareFontShape{X2}{cantarell-LF}{b}{sl}{<-> \cantarell@@scale Cantarell-Bold-X2-LF-Slanted}{}
\DeclareFontShape{X2}{cantarell-LF}{el}{sl}{<-> \cantarell@@scale Cantarell-Thin-X2-LF-Slanted}{}
\DeclareFontShape{X2}{cantarell-LF}{m}{sl}{<-> \cantarell@@scale Cantarell-Regular-X2-LF-Slanted}{}
