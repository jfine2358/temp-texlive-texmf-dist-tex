\ProvidesFile{il2mlmssq.fd}[2021/01/11 v1.0 Font defs for mlmodern]
\DeclareFontFamily{IL2}{mlmssq}{}
\DeclareFontShape{IL2}{mlmssq}{m}{n}
     {<-> cs-mlmssq8}{}
\DeclareFontShape{IL2}{mlmssq}{m}{sl}
     {<-> cs-mlmssqo8}{}
\DeclareFontShape{IL2}{mlmssq}{bx}{n}
     {<-> cs-mlmssqbx8}{}
\DeclareFontShape{IL2}{mlmssq}{bx}{sl}
     {<-> cs-mlmssqbo8}{}
%%%%%%% Font/shape undefined, therefore substituted
\DeclareFontShape{IL2}{mlmssq}{m}{it}
     {<->ssub*mlmssq/m/sl}{}
\DeclareFontShape{IL2}{mlmssq}{bx}{it}
     {<->ssub*mlmssq/bx/sl}{}
\DeclareFontShape{IL2}{mlmssq}{b}{n}
     {<->ssub * mlmssq/bx/n}{}
\DeclareFontShape{IL2}{mlmssq}{b}{sl}
     {<->ssub * mlmssq/bx/sl}{}
\DeclareFontShape{IL2}{mlmssq}{b}{it}
     {<->ssub * mlmssq/bx/it}{}
\endinput
