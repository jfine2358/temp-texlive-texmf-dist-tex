    test {\ifdefstring\blx@languagename{UKenglish}}%
    or
    test {\ifdefstring\blx@languagename{USenglish}}%
    or
    test {\ifdefstring\blx@languagename{australian}}%
    or
    test {\ifdefstring\blx@languagename{newzealand}}%
  }%
  {\@firstoftwo}%
  {\@secondoftwo}}

