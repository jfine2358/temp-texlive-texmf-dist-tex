\protected\def\cmsdateeraprintpre#1{% AD/BC order depends on language
  \ifdefstring\blx@dateera{astronomical}%
  {\iffieldnum{#1}%
    {\begingroup%
      \let\blx@tempa\@empty%
      \blx@ifstrendswith{#1}{endyear}%
      {\blx@stripfromendinto{#1}{endyear}\blx@datetype%
