\def\ProcessOptionsWithKV#1{%
  \let\@tempc\relax
  \let\DejaVuSerifCondensed@tempa\@empty
  \@for\CurrentOption:=\@classoptionslist\do{%
    \@ifundefined{KV@#1@\CurrentOption}%
    {}%
    {%
      \edef\DejaVuSerifCondensed@tempa{\DejaVuSerifCondensed@tempa,\CurrentOption,}%
      \@expandtwoargs\@removeelement\CurrentOption
        \@unusedoptionlist\@unusedoptionlist
    }%
  }%
  \edef\DejaVuSerifCondensed@tempa{%
    \noexpand\setkeys{#1}{%
      \DejaVuSerifCondensed@tempa\@ptionlist{\@currname.\@currext}%
    }%
  }%
  \DejaVuSerifCondensed@tempa
  \let\CurrentOption\@empty
}
\ProcessOptionsWithKV{DejaVuSerifCondensed}
\AtEndOfPackage{%
  \let\@unprocessedoptions\relax
}
    

