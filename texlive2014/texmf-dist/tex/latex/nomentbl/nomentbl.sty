      \large{\nomAname}}{%
      \ifthenelse{\equal{#1}{G}}{%
        \large{\nomGname}}{%
        \ifthenelse{\equal{#1}{X}}{%
          \large{\nomXname}}{%
          \ifthenelse{\equal{#1}{Z}}{%
            \large{\nomZname}}{%
            {}}}}}}

