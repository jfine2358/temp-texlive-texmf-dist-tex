    \def\scr@l@halign{l}%
    \edef\reserved@b{#5}%
    \expandafter\@tfor\expandafter\reserved@a\expandafter:\expandafter=%
    \reserved@b\do{%
      \if t\reserved@a
        \def\scr@l@valign{t}%
      \else
        \if c\reserved@a
          \def\scr@l@valign{c}%
          \def\scr@l@halign{c}%
        \else
          \if b\reserved@a
            \def\scr@l@valign{b}%
          \else
