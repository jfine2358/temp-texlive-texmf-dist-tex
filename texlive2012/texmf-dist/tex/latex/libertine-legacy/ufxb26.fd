}{}

\DeclareFontShape{U}{fxb26}{o}{n}{
   <-> \fxl@@scale fxbo-xb-26
}{}

\DeclareFontShape{U}{fxb26}{o}{it}{
   <-> \fxl@@scale fxboi-xb-26
}{}

\DeclareFontShape{U}{fxb26}{o}{b}{
   <-> \fxl@@scale fxbob-xb-26
}{}

\DeclareFontShape{U}{fxb26}{s}{n}{
