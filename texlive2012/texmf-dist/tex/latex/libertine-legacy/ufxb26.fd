\ProvidesFile{ufxb26.fd}
   [2011/06/06 scaled font definitions for libertine U/fxb26 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxb26}{}

\DeclareFontShape{U}{fxb26}{m}{n}{
   <-> \fxl@@scale fxbr-xb-26
}{}

