\ProvidesFile{ufxb24.fd}
   [2011/06/06 scaled font definitions for libertine U/fxb24 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxb24}{}

\DeclareFontShape{U}{fxb24}{m}{n}{
   <-> \fxl@@scale fxbr-xb-24
}{}

\DeclareFontShape{U}{fxb24}{m}{it}{
