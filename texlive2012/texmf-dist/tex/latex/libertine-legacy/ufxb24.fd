}{}

\DeclareFontShape{U}{fxb24}{b}{sl}{
   <-> \fxl@@scale fxbbo-xb-24
}{}

\DeclareFontShape{U}{fxb24}{o}{n}{
   <-> \fxl@@scale fxbo-xb-24
}{}

\DeclareFontShape{U}{fxb24}{o}{it}{
   <-> \fxl@@scale fxboi-xb-24
}{}

\DeclareFontShape{U}{fxb24}{o}{b}{
   <-> \fxl@@scale fxbob-xb-24
}{}

\DeclareFontShape{U}{fxb24}{s}{n}{
