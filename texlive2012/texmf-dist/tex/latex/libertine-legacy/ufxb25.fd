\ProvidesFile{ufxb25.fd}
   [2011/06/06 scaled font definitions for libertine U/fxb25 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxb25}{}

\DeclareFontShape{U}{fxb25}{m}{n}{
   <-> \fxl@@scale fxbr-xb-25
}{}

\DeclareFontShape{U}{fxb25}{m}{it}{
