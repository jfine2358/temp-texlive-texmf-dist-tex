}{}

\DeclareFontShape{U}{fxb25}{o}{n}{
   <-> \fxl@@scale fxbo-xb-25
}{}

\DeclareFontShape{U}{fxb25}{o}{it}{
   <-> \fxl@@scale fxboi-xb-25
}{}

\DeclareFontShape{U}{fxb25}{o}{b}{
   <-> \fxl@@scale fxbob-xb-25
}{}

\DeclareFontShape{U}{fxb25}{s}{n}{
