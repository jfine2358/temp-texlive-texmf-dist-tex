}{}

\DeclareFontShape{U}{fxbe3}{b}{sl}{
   <-> \fxl@@scale fxbbo-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{o}{n}{
   <-> \fxl@@scale fxbo-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{o}{it}{
   <-> \fxl@@scale fxboi-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{o}{b}{
   <-> \fxl@@scale fxbob-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{s}{n}{
   <-> \fxl@@scale fxbs-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{s}{it}{
