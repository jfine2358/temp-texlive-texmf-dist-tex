\ProvidesFile{ufxbe3.fd}
   [2011/06/06 scaled font definitions for libertine U/fxbe3 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxbe3}{}

\DeclareFontShape{U}{fxbe3}{m}{n}{
   <-> \fxl@@scale fxbr-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{m}{it}{
   <-> \fxl@@scale fxbri-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{m}{sl}{
   <-> \fxl@@scale fxbro-xb-e3
}{}

\DeclareFontShape{U}{fxbe3}{b}{n}{
