\DeclareFontShape{U}{fxbe3}{o}{sl}{<->ssub * fxbe3/o/it}{}
\DeclareFontShape{U}{fxbe3}{o}{bx}{<->ssub * fxbe3/o/b}{}
\DeclareFontShape{U}{fxbe3}{s}{sl}{<->ssub * fxbe3/s/it}{}
