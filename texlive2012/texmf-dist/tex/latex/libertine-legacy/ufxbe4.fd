}{}

\DeclareFontShape{U}{fxbe4}{o}{n}{
   <-> \fxl@@scale fxbo-xb-e4
}{}

\DeclareFontShape{U}{fxbe4}{o}{it}{
   <-> \fxl@@scale fxboi-xb-e4
}{}

\DeclareFontShape{U}{fxbe4}{o}{b}{
   <-> \fxl@@scale fxbob-xb-e4
}{}

\DeclareFontShape{U}{fxbe4}{s}{n}{
