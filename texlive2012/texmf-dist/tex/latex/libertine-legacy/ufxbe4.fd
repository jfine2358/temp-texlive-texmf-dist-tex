\ProvidesFile{ufxbe4.fd}
   [2011/06/06 scaled font definitions for libertine U/fxbe4 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxbe4}{}

\DeclareFontShape{U}{fxbe4}{m}{n}{
   <-> \fxl@@scale fxbr-xb-e4
}{}

\DeclareFontShape{U}{fxbe4}{m}{it}{
