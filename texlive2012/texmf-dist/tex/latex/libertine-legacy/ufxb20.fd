}{}

\DeclareFontShape{U}{fxb20}{o}{n}{
   <-> \fxl@@scale fxbo-xb-20
}{}

\DeclareFontShape{U}{fxb20}{o}{it}{
   <-> \fxl@@scale fxboi-xb-20
}{}

\DeclareFontShape{U}{fxb20}{o}{b}{
   <-> \fxl@@scale fxbob-xb-20
}{}

\DeclareFontShape{U}{fxb20}{s}{n}{
