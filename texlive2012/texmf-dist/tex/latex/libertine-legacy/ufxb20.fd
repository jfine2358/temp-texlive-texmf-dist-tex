\ProvidesFile{ufxb20.fd}
   [2011/06/06 scaled font definitions for libertine U/fxb20 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxb20}{}

\DeclareFontShape{U}{fxb20}{m}{n}{
   <-> \fxl@@scale fxbr-xb-20
}{}

\DeclareFontShape{U}{fxb20}{m}{it}{
