\ProvidesFile{ufxb1d.fd}
   [2011/06/06 scaled font definitions for libertine U/fxb1d (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxb1d}{}

\DeclareFontShape{U}{fxb1d}{m}{n}{
   <-> \fxl@@scale fxbr-xb-1d
}{}

\DeclareFontShape{U}{fxb1d}{m}{it}{
