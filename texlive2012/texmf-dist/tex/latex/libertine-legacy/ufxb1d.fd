}{}

\DeclareFontShape{U}{fxb1d}{o}{n}{
   <-> \fxl@@scale fxbo-xb-1d
}{}

\DeclareFontShape{U}{fxb1d}{o}{it}{
   <-> \fxl@@scale fxboi-xb-1d
}{}

\DeclareFontShape{U}{fxb1d}{o}{b}{
