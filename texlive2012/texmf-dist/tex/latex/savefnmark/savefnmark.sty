\newcommand\saveFN[1]{%
    \newcommand{#1}{}%
    \xdef#1{\noexpand\@useFN{\@mpfn}{\the\csname c@\@mpfn\endcsname}}%
    %\show#1%
}
\newcommand\useFN[1]{\begingroup\let\@footnotetext\relax#1\endgroup}
\newcommand\@useFN[2]{%
    \def\@mpfn{#1}%
    \expandafter\let\expandafter\thempfn\csname the#1\endcsname
    \@xfootnote[#2]%
}
