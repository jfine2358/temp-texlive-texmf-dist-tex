  }%
  \protected@edef#2{#1}\def#3{}%
  \begingroup
  \catcode`\-=12\relax
  \cptscantokens#2%
  \postgroupdef#2\endgroup
  \oifinsetTF{-}{#2}{%
    \expandafter\xwm@splitpagerange#2\xwm@nil{#2}{#3}%
  }{%
    \ifblankTF{#1}{%
      \xwm@err{Empty page range for watermark:
        ^^J'\expandcsonce\currwatermark'.^^J}\@ehc
    }{%
      \protected@edef#3{#1}%
    }%
