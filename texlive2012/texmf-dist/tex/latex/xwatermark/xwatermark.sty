\robust@def*\xwm@d@finecolorset#1(#2)#3{%
  \xwm@ifxcolorloaded{%
    \begingroup
    \cptemptifycsset{\xwm@tempb,\xwm@tempc,\xwm@tempd,\xwm@tempe}%
    \def\xwm@tempa##1,##2,##3,##4,##5\@nil{%
      \ifblankTF{##4}{%
