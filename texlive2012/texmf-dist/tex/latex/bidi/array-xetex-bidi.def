\def\endtabular{\endarray $\if@RTLtab\endR\fi\egroup\@RTLtabfalse}
\expandafter\let\csname endtabular*\endcsname=\endtabular
\def\@array[#1]#2{%
  \@tempdima \ht \strutbox
  \advance \@tempdima by\extrarowheight
