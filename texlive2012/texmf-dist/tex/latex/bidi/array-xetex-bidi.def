\def\@tabular{\if@RTL\@RTLtabtrue\fi%
  \leavevmode
  \hbox \bgroup\if@RTLtab\beginR \fi $\col@sep\tabcolsep \let\d@llarbegin\begingroup
                                    \let\d@llarend\endgroup
