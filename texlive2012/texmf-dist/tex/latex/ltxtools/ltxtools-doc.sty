  \def\LTS@s@venotes##1\sep##2\LTS@nil##3{%
    \def\nextnote{##2}%
    \edef\reserved@a{%
      \let\if\intitle\unexpanded{\intitle\cptswtrue}\else
      \unexpanded{\intitle\cptswfalse}\fi
    }%
