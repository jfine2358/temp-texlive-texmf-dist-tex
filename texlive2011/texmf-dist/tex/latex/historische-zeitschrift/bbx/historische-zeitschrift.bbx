\newbibmacro*{series+number}{%
 \iffieldundef{series}%
   {}
   {\printtext[parens]{%
    \printfield{series}%
    \iffieldundef{number}
      {}  
      {\setunit{\addcomma\space}%
       \bibstring{volume}~\printfield{number}}%
    \adddot}}}

