            \immediate\write\@auxout{\string
              \undonewlabel{#1}}%
          \fi%
        \fi%
        \immediate\write\@auxout{\string
          \newlabel{#1}{{}{#2}}}%
      \fi%
    \fi%
  \fi%
  \addtocounter{page}{+1}%
  }

