% definition without form:
\def\vpe@insertmark{%
  \vpe@annot{%
    /Subtype/Link%
    \ifx\vpe@border\vpe@borderdefault
    \else
      /Border[\vpe@border]%
    \fi
    \ifx\vpe@color\vpe@colordefault
    \else
      /C[\vpe@color]%
    \fi
    \ifnum\vpe@flag=0
    \else
      /F \vpe@flag
    \fi
    /A<<%
      /Type/Action%
      /S/Launch%
      \ifvpe@linux
        /F(\vpe@temp\vpe@removeslash\vpe@peekfile.vpe)%
      \else
