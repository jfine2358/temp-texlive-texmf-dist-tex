%*** keyval options
\def\vpe@revert#1#2{%
  \expandafter\ifx\csname vpe@#1\endcsname\relax
  \else
    \csname vpe@#1%
      \@gobble\iftrue\csname if#1\endcsname
        false%
      \else
        true%
      \fi
    \endcsname
  \fi
}

