\ProvidesFile{ufxl20.fd}
   [2010/08/30 scaled font definitions for libertine U/fxl20 (Nm).]

\expandafter\ifx\csname fxl@scale\endcsname\relax
   \let\fxl@@scale\@empty%
\else
   \edef\fxl@@scale{s*[\csname fxl@scale\endcsname]}%
\fi

\DeclareFontFamily{U}{fxl20}{}

\DeclareFontShape{U}{fxl20}{m}{n}{
   <-> \fxl@@scale fxlr-xl-20
}{}

\DeclareFontShape{U}{fxl20}{m}{sl}{
   <-> \fxl@@scale fxlro-xl-20
}{}

\DeclareFontShape{U}{fxl20}{m}{it}{
   <-> \fxl@@scale fxlri-xl-20
}{}

\DeclareFontShape{U}{fxl20}{b}{n}{
   <-> \fxl@@scale fxlb-xl-20
}{}

\DeclareFontShape{U}{fxl20}{b}{sl}{
   <-> \fxl@@scale fxlbo-xl-20
}{}

\DeclareFontShape{U}{fxl20}{b}{it}{
   <-> \fxl@@scale fxlbi-xl-20
}{}

\DeclareFontShape{U}{fxl20}{bx}{n}{<->ssub * fxl20/b/n}{}
\DeclareFontShape{U}{fxl20}{bx}{sl}{<->ssub * fxl20/b/sl}{}
\DeclareFontShape{U}{fxl20}{bx}{it}{<->ssub * fxl20/b/it}{}

\endinput
