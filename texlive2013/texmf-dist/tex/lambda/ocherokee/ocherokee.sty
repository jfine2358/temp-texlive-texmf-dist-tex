\IfFileExists{ot1uctt.fd}{% This is necessary, or else we get overful boxes.
   \def\ttdefault{uctt}}{}% Probably an Omega bug.
\ocp\InCherokee=cher2uni
\ocplist\InCherList=
    \addbeforeocplist 1 \InCherokee
    \nullocplist
\input lchenc.def
\newenvironment{cherokee}{\fontencoding{LCH}\selectfont%
                          \pushocplist\InCherList}{%
                          \popocplist}
\newenvironment{ucherokee}{\fontencoding{LCH}\selectfont}{}
\newcommand{\textcher}[1]{\leavevmode\begin{cherokee}#1\end{cherokee}}
\newcommand{\textucher}[1]{\begin{ucherokee}#1\end{ucherokee}}
\endinput
