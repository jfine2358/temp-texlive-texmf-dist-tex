\newcommand{\subtitle}[1]{%
  \posttitle{%
    \par\end{center}
    \begin{center}\large#1\end{center}
    \vskip1em}%
}

\newcommand{\TitlePage}[4]{

	\topskip0pt
	\vspace*{\fill}

	\begin{center}
	
	#1\\
	\vspace{3mm}

	#2\\
	\vspace{1cm}
	#3\\
	\vspace{1cm}
	#4
	
	\end{center}
	
	\vspace*{\fill}
	
	\newpage

}

\newcommand{\TitlePageGraphic}[6]{

	\topskip0pt
	\vspace*{\fill}

	\begin{center}
	
	\includegraphics[#6]{#5}
	\\\vspace{1cm}
	
	\end{center}
	
	\begin{center}
	
	\Large
	
	#1\\
	\vspace{3mm}
	\large
	#2\\
	\vspace{1cm}
	#3\\
	\vspace{1cm}
	#4
	
	\end{center}
	
	\vspace*{\fill}
	
	\newpage
}

\newcommand{\QuickTable}[3]{
	
	\begin{table}[#2]
	\caption{#1}
	\centering
	\begin{tabular}{#3}
	\hline\hline
}

\newcommand{\eQuickTable}{
	
	\end{tabular}
	\end{table}
}	

