%% required packages
\RequirePackage{atbegshi}[2007/04/19]% v1.2

%% variables
\let\bxpv@C@value\relax

%% \bxpv@C@bits
\@onlypreamble\bxpv@C@bits
\let\bxpv@C@bits\@empty

%% \bxpv@page@specials
