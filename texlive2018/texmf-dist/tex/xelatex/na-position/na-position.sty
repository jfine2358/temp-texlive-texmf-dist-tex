	\tkzTabLine{ , \myarg[1] , z , \myarg[2] , }
	\tkzTabLine{ ,\hskip -1.3cm (\Nline)\ \if +\myarg[1]\text{فوق}\else \if -\myarg[1]\text{تحت} \fi\fi \ (\Nplot)  ,   , \hskip 1cm (\Nline)\ \if +\myarg[2]\text{فوق}\else \if -\myarg[2]\text{تحت}\fi\fi  \ (\Nplot) , }
